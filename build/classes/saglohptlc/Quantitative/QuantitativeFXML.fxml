<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="saglohptlc.Quantitative.QuantitativeFXMLController">
   <children>
      <VBox layoutX="-2.0" prefHeight="406.0" prefWidth="160.0">
         <children>
            <Button mnemonicParsing="false" onAction="#onSettings" prefHeight="52.0" prefWidth="153.0" text="Settings" />
            <Button fx:id="loadimage" mnemonicParsing="false" onAction="#onLoadImage" prefHeight="57.0" prefWidth="152.0" text="Load Image" />
            <Button fx:id="qualitative" mnemonicParsing="false" onAction="#onQualitative" prefHeight="57.0" prefWidth="152.0" text="Qualitative" />
            <Button fx:id="quantitative" mnemonicParsing="false" prefHeight="57.0" prefWidth="152.0" text="Quantitative" />
            <Button fx:id="repo" mnemonicParsing="false" onAction="#onReports" prefHeight="57.0" prefWidth="152.0" text="Reports" />
            <Button fx:id="logout" mnemonicParsing="false" onAction="#logOut" prefHeight="57.0" prefWidth="152.0" text="Log out" />
            <Button fx:id="about" mnemonicParsing="false" onAction="#aboutUs" prefHeight="57.0" prefWidth="152.0" text="About us" />
         </children>
      </VBox>
      <StackPane fx:id="stackpane" layoutX="173.0" layoutY="58.0" prefHeight="291.0" prefWidth="416.0">
         <children>
            <ImageView fx:id="img" fitHeight="280.0" fitWidth="400.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </StackPane>
      <HBox layoutX="161.0" layoutY="10.0" prefHeight="41.0" prefWidth="434.0" spacing="10.0">
         <children>
            <Button mnemonicParsing="false" onAction="#retrieveImage1" prefHeight="31.0" prefWidth="143.0" text="Load Previous Image" />
            <Button mnemonicParsing="false" onAction="#onCrop" prefHeight="38.0" prefWidth="88.0" text="Crop" />
            <Button mnemonicParsing="false" onAction="#onAnalyse" prefHeight="38.0" prefWidth="88.0" text="Analyse" />
         </children>
         <padding>
            <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
         </padding>
      </HBox>
   </children>
</AnchorPane>
