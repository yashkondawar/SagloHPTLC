<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="imagecont" prefHeight="396.0" prefWidth="599.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="saglohptlc.Qualitative.QualitativeFXMLController">
   <children>
      <VBox prefHeight="396.0" prefWidth="148.0">
         <children>
            <Button mnemonicParsing="false" prefHeight="52.0" prefWidth="153.0" />
            <Button fx:id="loadimage" mnemonicParsing="false" onAction="#onLoadImage" prefHeight="57.0" prefWidth="152.0" text="Load Image" />
            <Button fx:id="qualitative" mnemonicParsing="false" prefHeight="57.0" prefWidth="152.0" text="Qualitative" />
            <Button fx:id="quantitative" mnemonicParsing="false" onAction="#onQuantitative" prefHeight="57.0" prefWidth="152.0" text="Quantitative" />
            <Button fx:id="repo" mnemonicParsing="false" onAction="#onReports" prefHeight="57.0" prefWidth="152.0" text="Reports" />
            <Button fx:id="logout" mnemonicParsing="false" onAction="#onLogOut" prefHeight="57.0" prefWidth="152.0" text="Log out" />
            <Button fx:id="about" mnemonicParsing="false" onAction="#onAboutUs" prefHeight="57.0" prefWidth="152.0" text="About us" />
         </children>
      </VBox>
      <MenuBar layoutX="169.0" layoutY="20.0" prefHeight="25.0" prefWidth="322.0">
        <menus>
          <Menu mnemonicParsing="false" text="Options">
            <items>
              <MenuItem fx:id="crop" mnemonicParsing="false" onAction="#onCrop" text="Crop" />
                  <MenuItem mnemonicParsing="false" onAction="#onClear" text="Clear" />
            </items>
          </Menu>
            <Menu mnemonicParsing="false" onAction="#retrieveImage1" text="Load Previous Image">
              <items>
                <MenuItem mnemonicParsing="false" text="Action 1" />
              </items>
            </Menu>
        </menus>
      </MenuBar>
      <StackPane fx:id="stackpane" layoutX="160.0" layoutY="48.0" prefHeight="332.0" prefWidth="419.0">
         <children>
            <ImageView fx:id="img" fitHeight="323.0" fitWidth="427.0" pickOnBounds="true" preserveRatio="true" />
         </children>
      </StackPane>
   </children>
</AnchorPane>
