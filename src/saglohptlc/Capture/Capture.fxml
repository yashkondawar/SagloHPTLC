<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="imagecont" prefHeight="396.0" prefWidth="599.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="saglohptlc.Capture.CaptureController">
   <children>
      <VBox prefHeight="396.0" prefWidth="148.0">
         <children>
            <Button fx:id="Settings" mnemonicParsing="false" onAction="#onSettings" prefHeight="52.0" prefWidth="153.0" text="Settings" />
            <Button mnemonicParsing="false" prefHeight="57.0" prefWidth="152.0" text="Load Image" />
            <Button fx:id="qualitative" mnemonicParsing="false" onAction="#onQualitative" prefHeight="57.0" prefWidth="152.0" text="Qualitative" />
            <Button fx:id="quantitative" mnemonicParsing="false" onAction="#onQuantitative" prefHeight="57.0" prefWidth="152.0" text="Quantitative" />
            <Button fx:id="repo" mnemonicParsing="false" onAction="#onReports" prefHeight="57.0" prefWidth="152.0" text="Reports" />
            <Button fx:id="logout" mnemonicParsing="false" onAction="#logOut" prefHeight="57.0" prefWidth="152.0" text="Log out" />
            <Button fx:id="about" mnemonicParsing="false" onAction="#aboutUs" prefHeight="57.0" prefWidth="152.0" text="About us" />
         </children>
      </VBox>
      <ImageView fx:id="img" fitHeight="298.0" fitWidth="409.0" layoutX="182.0" layoutY="76.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="187.0" layoutY="14.0" mnemonicParsing="false" onAction="#Browse" prefHeight="29.0" prefWidth="83.0" text="Browse" />
      <Button layoutX="287.0" layoutY="14.0" mnemonicParsing="false" onAction="#startCamera" prefHeight="29.0" prefWidth="111.0" text="Start Camera" />
      <Button layoutX="407.0" layoutY="14.0" mnemonicParsing="false" onAction="#Capture" prefHeight="29.0" prefWidth="83.0" text="Capture" />
      <Button layoutX="502.0" layoutY="14.0" mnemonicParsing="false" onAction="#Save" prefHeight="29.0" prefWidth="83.0" text="Save" />
   </children>
</AnchorPane>
